---
import Layout from '@/layouts/Layout.astro'
import WebinarArchiveHero from '@/components/webinar_archive/WebinarArchiveHero.astro'
import FilterForm from '@/components/common/FilterForm.astro'
import WebinarArchiveGrid from '@/components/webinar_archive/WebinarArchiveGrid.astro'
import ContactCTA from '@/components/common/ContactCTA.astro'
import { COMPANY_NAME } from '@/scripts/utils'
---

<Layout title={`Архив вебинаров - ${COMPANY_NAME}`} userType="agent" heroTextColor="dark">
    <WebinarArchiveHero userType="agent" />
    <FilterForm />
    <WebinarArchiveGrid userType="agent" />
    <ContactCTA title="Свяжитесь с нами для разработки персонального предложения" description="Наши менеджеры по работе с корпоративными клиентами готовы проконсультировать вас и подготовить коммерческое предложение, идеально соответствующее целям и бюджету вашей компании" />
</Layout>
